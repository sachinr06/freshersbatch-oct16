package HibernateMappingTest.HibernateMapping;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Table;

@Entity
@Table(name = "TEMPEMP")
public class Tempemp {

	@Column(name="ename")
	private String ename;
	
	@Column(name="mname")
	private String mname;
}
